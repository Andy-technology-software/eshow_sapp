.topView{
    display: flex;
    flex-direction: column;
    background-color: rgb(62, 62, 68);
    margin-top: 5%;
    margin-left: 5%;
    margin-right: 5%;
    border-radius: 6px;
    padding: 10px;
}

.title{
    color: white;
    font-size: 16;
}

.lineView{
    width: 80px;
    height: 1px;
    background-color: white;
    margin-top: 5px;
}

.des {
    margin-top: 5px;
    font-size: 14px;
    color: white;
    line-height: 20px;
}


