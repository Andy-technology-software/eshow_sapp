.topView{
    display: flex;
    flex-direction: column;
    background-color: rgb(62, 62, 68);
    margin-top: 5%;
    margin-left: 5%;
    margin-right: 5%;
    border-radius: 6px;
    padding: 10px;
}

.title{
    color: white;
    font-size: 16;
}

.lineView{
    width: 70px;
    height: 1px;
    background-color: white;
    margin-top: 5px;
}

.des {
    margin-top: 5px;
    font-size: 14px;
    color: white;
    line-height: 20px;
}


/*****展商*****/

.bottomView{
    display: flex;
    flex-direction: column;
    background-color: rgb(62, 62, 68);
    margin-top: 5%;
    margin-left: 5%;
    margin-right: 5%;
    border-radius: 6px;
    padding: 10px;
    margin-bottom: 5%;
}

.upView{
    display: flex;
    flex-direction: row;
    justify-content: space-between;
}

.leftView {
    display: flex;
    flex-direction: column;
}

.youImage{
    width: 20px;
    height: 20px;
}


/****列表****/
.listOutView{
    display: flex;
    flex-direction: column;
}

.listView{
    display: flex;
    flex-direction: row;
    margin-top: 10px;
    margin-bottom: 10px;
    margin-left: 10px;
    margin-right: 10px;
    
}

.listLeft{
    /*background-color: red;*/
    display: flex;
    flex-direction: column;
}

.listLeftImage {
    width: 60px;
    height: 60px;
}

.listRight{
    display: flex;
    flex-direction: column;
    margin-left: 5px;
    margin-right: 5px;
    justify-content: center;
    /*background-color: green;*/
}

.listRightTitle{
    font-size: 14px;
    color: white;
    margin-bottom: 5px;

}

.desText{
    color: rgb(175, 174, 174);
    font-size:12px;
    line-height:1.2;
    overflow:hidden;
    text-overflow:ellipsis;
    display:-webkit-box;
    -webkit-box-orient:vertical;
    -webkit-line-clamp:2
}

.bottomLineView{
    height: 1px;
    background-color: rgb(57, 57, 62);
}